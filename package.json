{
	"name": "auto",
	"version": "3.1.0",
	"description": "A code scanner Discord bot.",
	"main": "Auto.js",
	"scripts": {
		"test": "npm run lint",
		"lint": "eslint . --ext .js --ext .json",
		"start": "node Auto.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://bitbucket.org/dragonfire535/auto.git"
	},
	"keywords": [
		"bot",
		"commando",
		"discord",
		"discord-api",
		"discord-bot",
		"discord-js",
		"discord-js-commando"
	],
	"author": "dragonfire535 <danielbodendahl@gmail.com>",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://bitbucket.org/dragonfire535/auto/issues"
	},
	"homepage": "https://bitbucket.org/dragonfire535/auto#readme",
	"engines": {
		"node": ">=8.0.0"
	},
	"dependencies": {
		"common-tags": "^1.4.0",
		"discord.js": "github:hydrabolt/discord.js",
		"discord.js-commando": "github:gawdl3y/discord.js-commando",
		"erlpack": "github:discordapp/erlpack",
		"eslint": "^4.11.0",
		"eslint-config-aqua": "^2.0.1",
		"eslint-plugin-json": "^1.2.0",
		"js-beautify": "^1.7.4",
		"snekfetch": "^3.5.8",
		"to-markdown": "^3.1.0",
		"uws": "^8.14.1",
		"zlib-sync": "^0.1.3"
	},
	"eslintConfig": {
		"extends": "aqua",
		"plugins": [
			"json"
		],
		"rules": {
			"camelcase": "off",
			"id-length": "off",
			"no-await-in-loop": "off",
			"no-console": "off",
			"no-process-env": "off"
		}
	}
}
